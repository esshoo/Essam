{
  "hosting": {
    "public": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "functions/**"
    ],
    "headers": [
      {
        "source": "**/*.js",
        "headers": [{ "key": "Content-Type", "value": "text/javascript; charset=utf-8" }]
      }
    ]
  }
}
